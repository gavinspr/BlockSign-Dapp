      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "hardhat": "^2.9.3",
    "hardhat-spdx-license-identifier": "^2.0.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
